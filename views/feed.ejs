<%- include('partials/feedProfile') -%>
  <div class="container">
    <div class="row justify-content-center mt-5">
      <ul class="row list-unstyled">
        <% for(var i=0; i<posts.length; i++) {%>
          <li class="col-6 justify-content-between mt-5">
            <a href="/post/<%= posts[i]._id%>">
              <%if(posts[i].fileType == "audio"){ %>
                <h5><%= posts[i].title%>"</h5>
              <audio src="<%= posts[i].image%>" controls > </audio>
              <%}%>
              <%if(posts[i].fileType == "video"){ %>
                <h5><%= posts[i].title%>"</h5>
              <video src="<%= posts[i].image%>" controls > </video>
              <%}%>
              <%if(posts[i].fileType == "image"){ %>
                <h5><%= posts[i].title%>"</h5>
              <img class="img-fluid" src="<%= posts[i].image%>">
              <%}%>
            </a>
          </li>
        <% } %>
        </ul>
    </div>
  </div>  
  

<section id="picgallery">

  <div class="pic-ctn">
    
    <% for(var i=0; i<posts.length; i++) {%>
      <%if(posts[i].fileType == "image"){ %>
        
        <a href="/post/<%= posts[i]._id%>"></a>
        <img src="<%= posts[i].image%>" alt="" class="pic">
        
        
        <%}%>
        <%}%>
        
        
        
        
      </div>
    </section>
<%- include('partials/footer') -%>